{
  "compilerOptions": {
      "target": "ES2020",
    "module": "commonjs",
   "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./",              
    "sourceMap": true,
    "declaration": false,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
}
